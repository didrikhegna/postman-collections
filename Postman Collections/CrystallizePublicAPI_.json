          "_postman_id": "98806abf-5105-04e0-57d4-2188dc1de5a6",
          "name": "Create Order",
          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "var item = {\"subscription_id\": 15}",
                  "item = JSON.stringify(item)",
                  "postman.setEnvironmentVariable('itemData', item)"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "X-Crystallize-Tenant",
                "value": "Your_Tenant_Identifier"
              },
              {
                "key": "X-Crystallize-Token",
                "value": "Your_Tenant_Token"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \n    \"klarna_id\": \"provided_by_Klarna\",\n  \"first_name\": \"Test_first_name\",\n  \"last_name\": \"Test_last_name\",\n  \"billing_address\":\n   { \"given_name\": \"Test_first_name\",\n     \"family_name\": \"Test_last_name\",\n     \"street_address\": \"Test_street_address\",\n     \"postal_code\": \"Test_Code\",\n     \"city\": \"Test_City\",\n     \"country\": \"Test_Country\",\n     \"email\": \"test@crystallize.com\",\n     \"phone\": \"123456789\" },\n  \"shipping_address\":\n   { \"given_name\": \"Test_first_name\",\n     \"family_name\": \"Test_last_name\",\n     \"street_address\": \"Test_street_address\",\n     \"postal_code\": \"Test_Code\",\n     \"city\": \"Test_City\",\n     \"country\": \"Test_Country\",\n     \"email\": \"test@crystallize.com\",\n     \"phone\": \"123456789\" },\n    \"cart\": {\n        \"items\": [\n             \n            {\n                \"discount_rate\": 0,\n                \"name\": \"Item_name\",\n                \"quantity\": 1,\n                \"reference\": \"Item_SKU_Code-Variation\",\n                \"tax_rate\": 0,\n                \"total_price_excluding_tax\": 0,\n                \"total_price_including_tax\": 0,\n                \"total_tax_amount\": 0,\n                \"type\": \"shipping_fee\",\n                \"unit_price\": 0,\n                \"merchant_item_data\": {\n                  \"subscription_id\":1\n                }\n            },\n            {\n                \"discount_rate\": 0,\n                \"name\": \"Item_name_2\",\n                \"quantity\": 1,\n                \"reference\": \"Item_SKU_Code-Variation\",\n                \"tax_rate\": 0,\n                \"total_price_excluding_tax\": 0,\n                \"total_price_including_tax\": 0,\n                \"total_tax_amount\": 0,\n                \"type\": \"shipping_fee\",\n                \"unit_price\": 0,\n                \"merchant_item_data\": {\n                  \"subscription_id\":1\n            }\n        ],\n        \"total_price_excluding_tax\": 0,\n        \"total_tax_amount\": 0,\n        \"total_price_including_tax\": 0\n    }\n}"
            },
            "url": "{{protocol}}://{{url}}:{{port}}/v2/orders",
            "description": "Store an Order"
          },
          "response": []
